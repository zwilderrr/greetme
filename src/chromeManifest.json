{
  "manifest_version": 2,
  "name": "GreetMe - Unsplash photos, notes, goals",
  "short_name": "GreetMe",
  "description": "Goals, notes, and stunning Unsplash photos you choose, each and every tab.",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "./media/gm-icon-16.png",
    "32": "./media/gm-icon-32.png",
    "48": "./media/gm-icon-48.png",
    "128": "./media/gm-icon-128.png"
  },
  "browser_action": {
    "default_icon": "./media/gm-icon-16.png"
  },
  "permissions": ["tabs", "storage"],
  "content_security_policy": "script-src 'self' 'sha256-F61Itel0ZBdQI+q8Sge+7F0R739EX8Kee1TJxIR9RoY='; object-src 'self'"
}
